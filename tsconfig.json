{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "$app/*": ["app/*"],
      "$shared/*": ["shared/*"],
      "$features/*": ["features/*"],
      "$entities/*": ["entities/*"],
      "$widgets/*": ["widgets/*"],
      "$pages/*": ["pages/*"],
      "$assets/*": ["assets/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
